# ──────────────────────────────────────────────────────────────────────────────
# TickerIQ — secrets.toml template
# Copy this file to .streamlit/secrets.toml and fill in your real values.
# NEVER commit secrets.toml to GitHub (it is already in .gitignore).
# ──────────────────────────────────────────────────────────────────────────────

# ── personal branch (Railway) ─────────────────────────────────────────────────
# Login credentials for the app-level gate.
# On Railway, set these as environment variables instead:
#   AUTH_USERNAME, AUTH_PASSWORD
[auth]
username = "SamBa"
password = "change-this-to-a-strong-password"

# ── Supabase — https://supabase.com ───────────────────────────────────────────
# Free tier: Project Settings → API
# On Railway, set as: SUPABASE_URL, SUPABASE_ANON_KEY
[supabase]
url      = "https://YOUR_PROJECT_ID.supabase.co"
anon_key = "YOUR_ANON_PUBLIC_KEY"

# ── Cloudflare Turnstile (public branch only) ─────────────────────────────────
# Not needed on the personal branch — leave blank or omit entirely.
[turnstile]
site_key   = "YOUR_TURNSTILE_SITE_KEY"
secret_key = "YOUR_TURNSTILE_SECRET_KEY"

# ── Admin dashboard (public branch only) ──────────────────────────────────────
# Not used on the personal branch.
[admin]
password = "change-this-to-a-strong-password"
