# ──────────────────────────────────────────────────────────────────────────────
# TickerIQ — secrets.toml template
# Copy this file to .streamlit/secrets.toml and fill in your real values.
# NEVER commit secrets.toml to GitHub (it is already in .gitignore).
# ──────────────────────────────────────────────────────────────────────────────

# Supabase — https://supabase.com
# Free tier: Project Settings → API
[supabase]
url      = "https://YOUR_PROJECT_ID.supabase.co"
anon_key = "YOUR_ANON_PUBLIC_KEY"

# Cloudflare Turnstile — https://dash.cloudflare.com → Turnstile
# Use site key "1x00000000000000000000AA" for local testing (always passes)
[turnstile]
site_key   = "YOUR_TURNSTILE_SITE_KEY"
secret_key = "YOUR_TURNSTILE_SECRET_KEY"

# Admin dashboard password
[admin]
password = "change-this-to-a-strong-password"
